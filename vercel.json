{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    }, {
        "src": "./lokiloki/**",
        "use": "@vercel/static"
    } ],
    "routes": [{
        "src": "/(.*)",
        "dest": "index.js"
    }, {
        "src": "/",
        "dest": "lokiloki"
    }, {
        "src": "/(.*)",
        "dest": "lokiloki/$1"
    }]

}
